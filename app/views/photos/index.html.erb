<p id="notice"><%= notice %></p>

<h1>Listing Photos</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= link_to(image_tag(photo.image, width: 200), photo) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>

<%= link_to 'New Photo', new_photo_path %>
